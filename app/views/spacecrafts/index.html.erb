<% @spacecrafts.each do |spacecraft| %>

<div class="card-trip">
  <img src="https://as01.epimg.net/meristation/imagenes/2020/01/27/videos/1580120707_455030_1580120750_noticia_normal.jpg" />
  <div class="card-trip-infos">
    <div>
      <h2> Spaceship: <%= spacecraft.name %></h2>
      <h3>Rental cost: <%= spacecraft.price %></h3>
      <%= link_to "Show this spacecraft", spacecraft %>
      <br><br>
    </div>
    <h2 class="card-trip-pricing"></h2>
    <img src="data:image/jpeg;base64," class="card-trip-user avatar-bordered" />
  </div>
</div>
<% end %>
